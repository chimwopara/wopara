    <div id="fsConsole" class="fs-console">
        <div class="fs-console-header" onmousedown="DragManager.start('console', document.getElementById('fsConsole'), event)">
            <div class="fs-console-title">
                <span>‚ö°</span>
                <span>Fromsupply Console</span>
                <span id="fsConsoleContext" class="fs-console-path"></span>
            </div>
        </div>
        <input type="text" class="fs-console-input" id="fsConsoleInput" placeholder="Type command or 'help' for assistance..." />
        <div class="fs-console-output" id="fsConsoleOutput">
            <div class="fs-console-line">
                <span class="fs-console-success">Fromsupply Console v2.0 - Ready</span>
            </div>
            <div class="fs-console-line">
                <span class="fs-console-output-text">Type 'help' for commands or 'h' for shortcuts</span>
            </div>
        </div>
    </div>
